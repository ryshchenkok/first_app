@-moz-keyframes AnimateImage{
  from{
    opacity:1;
    -ms-filter:none;
    filter:none;
  }
  to{
    opacity:0;
    -ms-filter:"progid:DXImageTransform.Microsoft.Alpha(Opacity=0)";
    filter:alpha(opacity=0);
  }
}
@-webkit-keyframes AnimateImage{
  from{
    opacity:1;
    -ms-filter:none;
    filter:none;
  }
  to{
    opacity:0;
    -ms-filter:"progid:DXImageTransform.Microsoft.Alpha(Opacity=0)";
    filter:alpha(opacity=0);
  }
}
@-o-keyframes AnimateImage{
  from{
    opacity:1;
    -ms-filter:none;
    filter:none;
  }
  to{
    opacity:0;
    -ms-filter:"progid:DXImageTransform.Microsoft.Alpha(Opacity=0)";
    filter:alpha(opacity=0);
  }
}
