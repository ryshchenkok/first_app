        $(".hidden_attributes").show();
    } else {
        $(".hidden_attributes").hide();
    }
});
$(function () {
  $('body').on('submit', 'form', function () {
    var $form = $(this);
    $form.on('submit.propagate', function () {
      return false;
    });
    setTimeout(function () {
      $form.off('submit.propagate')
    }, 1000);
  });
});
