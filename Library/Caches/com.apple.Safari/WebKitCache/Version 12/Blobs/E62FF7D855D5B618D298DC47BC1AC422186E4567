// Fix bug with caching response and restoring tabs
// More info http://stackoverflow.com/questions/1013637/unexpected-caching-of-ajax-results-in-ie8
$.ajaxSetup({cache: false});

var App = {
  Classes: {},

  Features: {
    init: function() {
      var features = $('body').data('features');

      if (features) {
        var featuresHash = features.trim().split(' ').reduce(function(acc, feature) {
          acc[feature] = '/assets/features/' + feature + '.js';
          return acc;
        }, {});

        yepnope([{
          load: featuresHash,
          callback: function (path, _, name) {
            if(App.Features[name] && typeof App.Features[name].init === 'function') {
              App.Features[name].init();
            }
          }
        }]);
      }
    },

    time_tracking: {
      init: function() {
        window.employeePage = new App.Classes.Page();
        window.employeePage.week(new App.Classes.WeekModel({
          afterLoad: function() {
            ko.applyBindings(window.employeePage, $('#main').get(0));
            App.Features.time_tracking.ready = true;
          }
        }));
      },

      ready: false
    },

    flash: {
      init: function() {
        ko.applyBindings(new App.Classes.Flash(), $('#flashMessages').get(0));
      }
    },

    project_assignments: {
      init: function() {
        ko.applyBindings(new App.Classes.ProjectAssignments(employee_positions), $('[data-project-assignments]').get(0));
      }
    },

    new_project: {
      init: function() {
        ko.applyBindings(new App.Classes.NewProject(), $('#new_project').get(0));
      }
    }
  }
};

var Story = {
  init: function () {
    this.bindings();
  },
  bindings: function () {
    $(".estimateTimeControls :radio").bind('change', this.changeEstimateTimeType);
    $('#story_estimate').bind('focus', this.autocheckEstimateTimeType);
    $('#sidebar #story_with_questions_form :checkbox').live('change', this.storyWithQuestionsChange);

    $('#new_file_attachment_toggler, .file_attachment_toggler').live('click', this.toggler.newAttachment);
    $('#attachments_toggler').live('click', this.toggler.attachments);
    $('.new_task_toggler, #newTaskToggler, #newTaskWrapper a[data-id=close-form]').bind('click', this.toggler.newTask);
  },
  changeEstimateTimeType: function () {
    if ($(this).attr('id') === 'story_estimate_by_tasks_false') {
      $('#story_estimate').focus();
    }
  },
  autocheckEstimateTimeType: function () {
    $(this).closest('#estimateByTime').find('#story_estimate_by_tasks_false').prop('checked', true);
  },
  storyWithQuestionsChange: function () {
    $(this).closest('form').submit();
  },
  toggler: {
    newAttachment: function () {
      $('#new_file_attachment_toggler').toggle();
      $('#newFileAttachmentWrapping').toggle();
      $('#new_file_attachment').map(function() { this.reset(); });
      return false;
    },
    attachments: function () {
      $('#attachments').toggle();
    },
    newTask: function () {
      $('#supportedFormatsHelp').hide();
      $('#newTaskToggler').toggle();
      $('#newTaskWrapper').toggle();
      $('#new_task').resetForm();
      $('input#task_name').focus();
      return false;
    }
  }
};

var Task = {
  init: function () {
    this.remainingTimeSpan = $('#remainingTime');
    this.remainingTimeForm = this.remainingTimeSpan.nextAll('form');
    this.remainingTimeInput = this.remainingTimeForm.find(':text');
    this.cancel = $('#cancel_remaining_time_edit');

    this.remainingTimeSpan.bind('click', $.proxy(this.showRemainingTimeForm, this));
    this.cancel.bind('click', $.proxy(this.hideRemainingTimeForm, this));

    $('#editTaskTimes').bind('click', this.showTaskTimesForm);
    $('.hideTaskTimes').live('click', this.hideTaskTimesForm);

    $('.editForm > form a[data-id=close-form]').bind('click', this.cancelEditTask);
  },
  showRemainingTimeForm: function () {
    this.remainingTimeSpan.hide();
    this.remainingTimeForm.show();
    this.value = this.remainingTimeInput.focus().val();
  },
  hideRemainingTimeForm: function () {
    this.remainingTimeInput.val(this.value);
    this.remainingTimeForm.hide();
    this.remainingTimeSpan.show();
  },
  showTaskTimesForm: function () {
    $('#task_times_box').hide();
    $('.task_times_form').show();
    if ($('.task_times_form').is(':visible')) {
      $('.task_times_form').find('textarea').focus();
    }
    return false;
  },
  hideTaskTimesForm: function () {
    $('.task_times_form').hide();
    $('#task_times_box').show();
    $('.story-times .actual').filter('.selected').removeClass('selected');
    return false;
  },
  cancelEditTask: function () {
    document.location.href = $('#cancel_path').val();
    return false;
  }
};

var References = {
  init: function () {
    $('.link_to_textile_reference').bind('click', this.textile);
    $('.show_formats_help').bind('click', this.formats);
  },
  textile: function () {
    window.open('/textileReference.html','new','width=500,height=600,scrollbars=yes,resizable=yes');
  },
  formats: function () {
    $('#supportedFormatsHelp').toggle();
    return false;
  }
};

// Send attachments to textarea when click
var Attachments = {
  init: function () {
    this.textarea = $('.editForm .textareaBox textarea');
    if (this.textarea.length === 0) return true;
    this.title = $('#story_name');
    $('#attachments').on('click', '.attachmentItem:not(.draggable-area)', this.add);
    position = this.textarea.val().length;
    Attachments.textarea.setSelection(position,position);
    Attachments.title.focus();
  },
  add: function (e) {
    var url = $(this).data('url'),
        contentType = $(this).data('type'),
        name = $(this).data('name'),
        textileUrl = Attachments.textileUrl(url, contentType, name);
    if ((location.href.match(/\/\d+\/edit/) || location.href.match(/\/stories\/new/)) && e.target.className !== 'delete') {
      Attachments.textarea.replaceSelectedText(textileUrl);
      return false;
    }
  },
  textileUrl: function (url, contentType, name) {
    if(contentType.match("image\/")){
      return "!(popup)"+ url + "!:" + url.replace('/thumb/', '/original/') + "\n";
    } else {
      return "\"" + name + "\":" + url + "\n";
    }
  }
};

var Project = {
  init: function () {
    this.inputs = $(".project-digest input:radio");
    this.inputs.not(":checked").parent().find('select').attr('disabled', 'disabled');
    this.bindings();
    this.readonly = $('body').data('readonly');
  },
  bindings: function () {
    this.inputs.on('click', this.processDigest);
    $('.update-status-remote input:checkbox').on('change', this.processStatus);
  },
  processDigest: function () {
    $li = $(this).parent();
    $li.find('select').removeAttr('disabled');
    $li.siblings().find('select').attr('disabled', 'disabled');
  },
  processStatus: function () {
    $(this).closest('form').submit();
  }
};

var Invoice = {
  init: function () {
    $('.set-bill-remote input:checkbox').on('change', this.processStatus);
  },
  processStatus: function () {
    $(this).closest('form').submit();
  }
};

var Users = {
  init: function () {
    this.initTableSorter();
  },
  initTableSorter: function () {
    $("#tablesorter").tablesorter();
  }
};

var Customers = {
  init: function () {
    this.bindings();
  },
  bindings: function () {
    $('.project-customers a').on('click', function (e) {
      e.preventDefault();
      var text = $(this).parent().data('customers');
      $(this).parent().html(text);
    });
  }
};

var SuperadminPanel = {
  init: function () {
    this.bindings();
  },
  bindings: function () {
    $("form.update-superadmin-status input:checkbox").on('change', function(){
      $(this).closest("form").submit();
    });
  }
}

$(function() {
  App.Features.init();
  Story.init();
  Task.init();
  References.init();
  Attachments.init();
  Project.init();
  Invoice.init();
  Customers.init();
  Users.init();
  SuperadminPanel.init();

  $("#actual_tasks .name").bind('click', function() {
    var next = $(this).parent().next();
    $('div[id^=task_times_form]').not(next).hide();
    next.toggle();
  });

  //FIXME: customization for textile
  $('#description img.popup').parent().attr('target', '_blank');
  $("#description table tbody tr:odd").addClass("even");

  $('#template_description_toggler').bind('click', function () {
    $('#template_description').toggle();
    return false;
  });

  if (location.href.match(/stories\/\d*\/edit/) || location.href.match(/stories\/new/)) $('#story_name').focus();
});
$(function(){
  JST = {};

  $("script[type='text/template']").each(function () {
    var dataId = $(this).data('id');
    if (dataId) {
      dataId = dataId.replace('-template', "");
      JST[dataId] = _.template($(this).html());
    }
  })
});
(function() {
  $(function() {
    return $('.operations').detailsLink();
  });

}).call(this);
(function() {
  var bind = function(fn, me){ return function(){ return fn.apply(me, arguments); }; };

  App.Classes.FileUploader = (function() {
    function FileUploader() {
      this.prependToDescription = bind(this.prependToDescription, this);
      this.add = bind(this.add, this);
      this.complete = bind(this.complete, this);
      this.send = bind(this.send, this);
      this.bindings = bind(this.bindings, this);
      this.url = $('#upload_url').val();
      if (!Project.readonly) {
        this.bindings();
      }
      this.flag = true;
      this.count = 0;
      this.filesToPrepend = [];
    }

    FileUploader.prototype.bindings = function() {
      $('.draggable-area').fileupload({
        url: this.url,
        dataType: 'script',
        paramName: 'file_attachment[]',
        fileInput: $('.add_file_form input:file'),
        dropZone: $('.draggable-area'),
        send: this.send,
        done: this.complete,
        fail: this.complete,
        add: this.add,
        sequentialUploads: true
      });
      $(window).on('drag', (function(_this) {
        return function(e) {
          _this.flag = false;
          return true;
        };
      })(this));
      $(window).on('dragover', (function(_this) {
        return function(e) {
          if (!_this.flag) {
            return true;
          }
          if (e.target.id === 'story_description') {
            $('#story_description').addClass('selected');
          }
          $('.draggable-area').fileupload('option', 'dropZone', $(window));
          $('.attachment-wrapper').show();
          $('#attachments').show();
          return $('#attachments').find('.draggable-area').addClass('selected');
        };
      })(this));
      $(window).on('dragleave drop', function(e) {
        $('#attachments').find('.draggable-area').removeClass('selected');
        if (e.target.id === 'story_description') {
          return $('#story_description').removeClass('selected');
        }
      });
      return $(window).on('drop', (function(_this) {
        return function(e) {
          $('.draggable-area').fileupload('option', 'dropZone', $('.draggable-area'));
          return _this.flag = true;
        };
      })(this));
    };

    FileUploader.prototype.send = function(e, data) {
      this.count++;
      $('.draggable-area .wait').show();
      return $('.draggable-area span').hide();
    };

    FileUploader.prototype.complete = function() {
      this.count--;
      if (this.count === 0) {
        $('.draggable-area .wait').hide();
        return $('.draggable-area span').show();
      }
    };

    FileUploader.prototype.add = function(e, data) {
      if (e.target.id === 'story_description') {
        this.filesToPrepend.push(data.files[0].name.replace(/\s/g, "_"));
      }
      return data.submit();
    };

    FileUploader.prototype.prependToDescription = function(name) {
      var position;
      position = $('#story_description').val().length;
      if (_(this.filesToPrepend).include(name)) {
        this.filesToPrepend = _(this.filesToPrepend).without(name);
        $('#story_description').setSelection(position, position);
        return $('#attachments').find("div[data-name='" + name + "']").not('.removed').trigger('click');
      }
    };

    return FileUploader;

  })();

}).call(this);
(function() {
  var bind = function(fn, me){ return function(){ return fn.apply(me, arguments); }; };

  App.Classes.History = (function() {
    function History() {
      this.pop = bind(this.pop, this);
      this.replace = bind(this.replace, this);
      this.push = bind(this.push, this);
      this.popped = false;
      this.initialURL = location.href;
    }

    History.prototype.exists = function() {
      return history && history.pushState;
    };

    History.prototype.push = function(url) {
      if (this.exists()) {
        return history.pushState(null, document.title, url);
      }
    };

    History.prototype.replace = function(url) {
      if (this.exists()) {
        return history.replaceState(null, document.title, url);
      }
    };

    History.prototype.pop = function(callback) {
      var initialPop, params;
      initialPop = !this.popped && location.href === this.initialURL;
      this.popped = true;
      if (initialPop) {
        return;
      }
      if (this.exists()) {
        params = $.deparam.querystring();
        return callback(params);
      }
    };

    return History;

  })();

}).call(this);
(function() {
  App.Classes.InvoiceLoader = (function() {
    function InvoiceLoader() {}

    InvoiceLoader.prototype.show = function($invoiceDiv) {
      var $editForm;
      $editForm = $invoiceDiv.find('.editForm');
      $editForm.prepend("<div class='loader'><div class='loader-image'></div></div>");
      return $editForm.css('opacity', 0.5);
    };

    return InvoiceLoader;

  })();

  $(function() {
    App.invoiceLoader = new App.Classes.InvoiceLoader;
    return $('#invoices').on('click', '.reload-invoice', function(e) {
      return App.invoiceLoader.show($(e.target).closest('.project'));
    });
  });

}).call(this);
(function() {
