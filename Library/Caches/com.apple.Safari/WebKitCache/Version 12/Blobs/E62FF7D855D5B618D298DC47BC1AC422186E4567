    return $('.invoices-next').detailsLink();
  });

}).call(this);
(function() {
  $(function() {
    return $('.project-payments').detailsLink();
  });

}).call(this);
(function() {
  App.ProjectsSelector = {
    bind: function() {
      var that;
      that = this;
      that.projects = $('.projects-list').find('ul').find('a');
      that.inactive = that.projects.parents("ul").filter('.inactive').find('a');
      that.current = $('#project-selector').find('span').text();
      that.projects.filter(function() {
        return this.text === that.current;
      }).addClass('active');
      $(document).bind('keydown', function(e) {
        return that.checkKey(e.keyCode);
      });
      return that.projects.bind('hover', function() {
        return that.projects.filter(".active").each(function() {
          return $(this).removeClass('active');
        });
      });
    },
    unbind: function() {
      $(document).unbind('keydown');
      return this.projects.removeClass('active');
    },
    checkKey: function(key) {
      this.current = this.projects.filter('.active');
      if ($('.projects-list').is(':visible')) {
        switch (key) {
          case 13:
            return this.projects.filter('.active')[0].click();
          case 37:
            return this.horizontal('prev');
          case 38:
            return this.vertical('prev');
          case 39:
            return this.horizontal('next');
          case 40:
            return this.vertical('next');
          default:
            return this.find(key);
        }
      }
    },
    horizontal: function(direction) {
      var index;
      index = this.current.parent().index();
      if (this.current.parents('ul')[direction]().filter('ul')[0] !== void 0) {
        if (this.current.parents('ul')[direction]().children().length <= index) {
          index = 'last';
        } else {
          index = "eq(" + (this.current.parent().index()) + ")";
        }
        return this.current.removeClass('active').parents('ul')[direction]().find("a:" + index).addClass('active');
      } else if (this.current.parents(".projects-block")[direction]().hasClass('projects-block')) {
        if (this.current.parents('.projects-block')[direction]().find("ul").children().length <= index) {
          index = 'last';
        } else {
          index = "eq(" + (this.current.parent().index()) + ")";
        }
        return this.current.removeClass('active').parents(".projects-block")[direction]().find("a:" + index).addClass('active');
      }
    },
    vertical: function(direction) {
      var position;
      position = direction === 'next' ? 'first' : 'last';
      if (this.current.parent()[direction]().find('a')[0] !== void 0) {
        this.current.removeClass('active').parent()[direction]().find('a').addClass('active');
      } else if (this.current.parents('ul')[direction]().filter('ul')[0] !== void 0) {
        this.current.removeClass('active').parents("ul")[direction]().find("a:" + position).addClass('active');
      } else if (this.current.parents('.projects-block')[direction]().children('ul')[0] !== void 0) {
        this.current.removeClass('active').parents(".projects-block")[direction]().find("ul:" + position).find("a:" + position).addClass('active');
      }
      return false;
    },
    find: function(key) {
      var char;
      char = String.fromCharCode(key);
      this.projects.each(function() {
        return $(this).removeClass('active');
      });
      if ($('.projects-list').find('ul').find('a:hover')[0] === void 0) {
        return this.inactive.each(function() {
          if ($(this).text()[0] === char) {
            $(this).addClass('active');
            return false;
          }
        });
      }
    }
  };

  _.bindAll(App.ProjectsSelector);

}).call(this);
(function() {
  var bind = function(fn, me){ return function(){ return fn.apply(me, arguments); }; },
    indexOf = [].indexOf || function(item) { for (var i = 0, l = this.length; i < l; i++) { if (i in this && this[i] === item) return i; } return -1; };

  App.Classes.StoryFilter = (function() {
    function StoryFilter(manager) {
      this.manager = manager;
      this.isStoryVisible = bind(this.isStoryVisible, this);
      this.updateSidebar = bind(this.updateSidebar, this);
      this.findsVisible = bind(this.findsVisible, this);
      this.recalculate = bind(this.recalculate, this);
      this.hidePluses = bind(this.hidePluses, this);
      this.hideUnnecessary = bind(this.hideUnnecessary, this);
      this.hideEmpty = bind(this.hideEmpty, this);
      this.filter = bind(this.filter, this);
      this.setStatuses = bind(this.setStatuses, this);
      this.setTags = bind(this.setTags, this);
      this.setUsers = bind(this.setUsers, this);
      this.json = App.stories;
    }

    StoryFilter.prototype.setUsers = function(users) {
      this.users = users;
      return this;
    };

    StoryFilter.prototype.setTags = function(tags) {
      this.tags = tags;
      return this;
    };

    StoryFilter.prototype.setStatuses = function(statuses) {
      this.statuses = statuses;
      return this;
    };

    StoryFilter.prototype.filter = function() {
      var filteredStoriesIds;
      filteredStoriesIds = this.findsVisible();
      $(".story-list ul li:not(.add)").hide().each(function(index, el) {
        var $el;
        $el = $(el);
        if (_.include(filteredStoriesIds, Number($el.data('id')))) {
          return $el.show();
        }
      });
      $('.stories-period').show();
      this.hideUnnecessary();
      this.hidePluses();
      this.hideEmpty();
      return this.recalculate();
    };

    StoryFilter.prototype.hideEmpty = function() {
      var ref;
      if (((ref = this.manager.groupedBy) !== 'status' && ref !== 'periods') && (this.manager.isArchive() || this.manager.isFiltered())) {
        $('.stories-period:not(:has(li:visible:not(.add)))').hide();
      }
      if (this.manager.groupedBy === 'users') {
        return $('.stories-period:not(:has(li:visible:not(.add)))').has('h2 [data-active=\'false\']').hide();
      }
    };

    StoryFilter.prototype.hideUnnecessary = function() {
      var $groups, ar;
      $groups = $('.stories-period');
      ar = null;
      if (this.manager.groupedBy === 'users' && this.manager.users.length > 0) {
        ar = this.manager.users;
      }
      if (this.manager.groupedBy === 'tags' && this.tags.length > 0) {
        ar = this.tags;
      }
      if (ar) {
        return $groups.hide().find('ul').filter(function() {
          var ref;
          return ref = $(this).data('group').toString(), indexOf.call(ar, ref) >= 0;
        }).closest('.stories-period').show();
      }
    };

    StoryFilter.prototype.hidePluses = function() {
      if (!this.manager.isArchive()) {
        $('#planned-stories li.add').toggle(!this.manager.isFiltered());
      }
      return $('#planned-stories .stories-period:has(h2 [data-active=\'false\'])').find('li.add').hide();
    };

    StoryFilter.prototype.recalculate = function() {
      if (this.manager.groupedBy !== 'ungrouped') {
        return $('.story-list ul:visible').calculate({
          manager: this.manager
        });
      }
    };

    StoryFilter.prototype.findsVisible = function() {
      this.filteredStories = _.select(App.stories, (function(_this) {
        return function(el) {
          return _this.isStoryVisible(el);
        };
      })(this));
      return _.pluck(this.filteredStories, 'id');
    };

    StoryFilter.prototype.updateSidebar = function() {
      var $tags, $users, archive, stories, uniqTags, uniqUsers;
      this.userFilter || (this.userFilter = $('#users_filter').prevUntil('.group').andSelf());
      this.statusFilter || (this.statusFilter = $('#status-filter').prevUntil('#tag_filter').andSelf());
      this.tagsFilter || (this.tagsFilter = $('#tag_filter').prevUntil('#users_filter').andSelf());
      this.usersFilter || (this.usersFilter = $('#users_filter').prevUntil('ul.group').andSelf());
      if (this.manager.isArchive()) {
        archive = true;
        this.statusFilter.hide();
      } else {
        archive = false;
        this.statusFilter.show();
      }
      stories = _(App.stories).select(function(story) {
        return story.archive === archive;
      });
      uniqUsers = _(stories).chain().pluck('users').flatten().uniq().value();
      uniqTags = _(stories).chain().pluck('tags').flatten().uniq().value();
      this.tagsFilter.toggle(uniqTags.length > 0);
      this.usersFilter.toggle(uniqUsers.length > 0);
      $users = $('#users_filter>ul>li').hide().filter(function() {
        var ref;
        return ref = $(this).data('login'), indexOf.call(uniqUsers, ref) >= 0;
      }).show();
      return $tags = $('#tag_filter>ul>li').hide().filter(function() {
        var ref;
        return ref = $(this).data('name').toString(), indexOf.call(uniqTags, ref) >= 0;
      }).show();
    };

    StoryFilter.prototype.isStoryVisible = function(story) {
      var archive;
      archive = this.manager.isArchive();
      if (story.archive !== archive) {
        return false;
      }
      if (_.any(this.statuses) && !_.include(this.statuses, story.status)) {
        return false;
      }
      if (_.any(this.users) && _.intersection(this.users, story.users).length === 0) {
        return false;
      }
      if (_.any(this.tags) && _.intersection(this.tags, story.tags).length === 0) {
        return false;
      }
      return true;
    };

    return StoryFilter;

  })();

}).call(this);
(function() {
  var bind = function(fn, me){ return function(){ return fn.apply(me, arguments); }; },
    indexOf = [].indexOf || function(item) { for (var i = 0, l = this.length; i < l; i++) { if (i in this && this[i] === item) return i; } return -1; };

  App.Classes.Grouper = (function() {
    function Grouper(manager) {
      this.manager = manager;
      this.sortId = bind(this.sortId, this);
      this.convertPeriod = bind(this.convertPeriod, this);
      this.title = bind(this.title, this);
      this.emptyPeriod = bind(this.emptyPeriod, this);
      this.addEmptyStatuses = bind(this.addEmptyStatuses, this);
      this.addEmptyUsers = bind(this.addEmptyUsers, this);
      this.addEmptyPeriods = bind(this.addEmptyPeriods, this);
      this.minPeriod = bind(this.minPeriod, this);
      this.maxPeriod = bind(this.maxPeriod, this);
      this.addEmpty = bind(this.addEmpty, this);
      this.group = bind(this.group, this);
      this.setOptions = bind(this.setOptions, this);
      this.draw = bind(this.draw, this);
      this.regroup = bind(this.regroup, this);
      String.prototype.ucfirst = function() {
        return this.charAt(0).toUpperCase() + this.slice(1);
      };
    }

    Grouper.prototype.regroup = function() {
      var archive;
      archive = this.manager.isArchive();
      this.revertPeriod = this.manager.isArchive() ? -1 : 1;
      this.stories = _(App.stories).select(function(story) {
        return story.archive === archive;
      });
      this.draw();
      if (!Project.readonly) {
        return App.sortable.setup();
      }
    };

    Grouper.prototype.draw = function() {
      var groups, lastGroup;
      this.setOptions();
      groups = lastGroup = "";
      _(this.options).each((function(_this) {
        return function(option) {
          if (option.dataGroup === '0' || option.dataGroup === '') {
            return lastGroup = JST['group'](option);
          } else {
            return groups += JST['group'](option);
          }
        };
      })(this));
      return $('#planned-stories').html(groups + lastGroup);
    };

    Grouper.prototype.setOptions = function() {
      var isArchive, keys;
      this.group();
      keys = _(this.groupedStories).keys();
      this.options = [];
      isArchive = this.manager.isArchive();
      _(keys).each((function(_this) {
        return function(key) {
          return _this.options.push({
            story_ids: _(_this.groupedStories["" + key]).pluck('id'),
            title: _this.title(key),
            dataGroup: key,
            sortID: _this.sortId(key),
            addButton: !(Project.readonly || isArchive || key === 'Overdue' || _this.manager.groupedBy === 'status')
          });
        };
      })(this));
      this.addEmpty();
      return this.options = _(this.options).sortBy(function(option) {
        return option.sortID;
      });
    };

    Grouper.prototype.group = function() {
      var date, logins;
      switch (this.manager.groupedBy) {
        case 'status':
          return this.groupedStories = _(this.stories).groupBy((function(_this) {
            return function(story) {
              return story["status"];
            };
          })(this));
        case 'periods':
          this.groupedStories = {};
          if (this.manager.isArchive()) {
            _(this.stories).each((function(_this) {
              return function(story) {
                if (_this.groupedStories["" + story.period] == null) {
                  _this.groupedStories["" + story.period] = [];
                }
                return _this.groupedStories["" + story.period].push(story);
              };
            })(this));
          } else {
            date = Date.today().monday().add(-7).days();
            _(this.stories).each((function(_this) {
              return function(story) {
                if (story.period !== 0) {
                  if (Date.parse(story.period) >= date) {
                    if (_this.groupedStories["" + story.period] == null) {
                      _this.groupedStories["" + story.period] = [];
                    }
                    return _this.groupedStories["" + story.period].push(story);
                  } else {
                    if (_this.groupedStories["Overdue"] == null) {
                      _this.groupedStories["Overdue"] = [];
                    }
                    return _this.groupedStories["Overdue"].push(story);
                  }
                } else {
                  if (_this.groupedStories["0"] == null) {
                    _this.groupedStories["0"] = [];
                  }
                  return _this.groupedStories["0"].push(story);
                }
              };
            })(this));
          }
          return this.groupedStories;
        case 'tags':
          this.groupedStories = {};
          _(this.stories).each((function(_this) {
            return function(story) {
              if (story.tags.length === 0) {
                if (_this.groupedStories[""] == null) {
                  _this.groupedStories[""] = [];
                }
                return _this.groupedStories[''].push(story);
              } else {
                return _(story.tags).each(function(name) {
                  if (_this.groupedStories["" + name] == null) {
                    _this.groupedStories["" + name] = [];
                  }
                  return _this.groupedStories["" + name].push(story);
                });
              }
            };
          })(this));
          if (!this.groupedStories['']) {
            this.groupedStories[''] = [];
          }
          return this.groupedStories;
        case 'users':
          logins = _(App.users).pluck('login');
          this.groupedStories = {};
          _(this.stories).each((function(_this) {
            return function(story) {
              if (!story.users.length) {
                if (_this.groupedStories[""] == null) {
                  _this.groupedStories[""] = [];
                }
                return _this.groupedStories[''].push(story);
              } else {
                return _(story.users).each(function(login) {
                  if (indexOf.call(logins, login) < 0) {
                    login = '';
                  }
                  if (_this.groupedStories["" + login] == null) {
                    _this.groupedStories["" + login] = [];
                  }
                  return _this.groupedStories["" + login].push(story);
                });
              }
            };
          })(this));
          if (!this.groupedStories['']) {
            this.groupedStories[''] = [];
          }
          return this.groupedStories;
        default:
          return this.groupedStories = {
            '0': this.stories
          };
      }
    };

    Grouper.prototype.addEmpty = function() {
      if (!this.options) {
        this.options = [];
      }
      if (!(Project.readonly || this.manager.isArchive())) {
        switch (this.manager.groupedBy) {
          case 'periods':
            return this.addEmptyPeriods();
          case 'users':
            return this.addEmptyUsers();
          case 'status':
            return this.addEmptyStatuses();
        }
      }
    };

    Grouper.prototype.maxPeriod = function() {
      var lastMonday, max;
      lastMonday = Date.today().monday().add(-7).days();
      if (this.options.length === 0) {
        return lastMonday;
      }
      max = _(this.options).chain().pluck('dataGroup').max(function(dataGroup) {
        if (dataGroup === '0' || dataGroup === 'Overdue') {
          return Date.parse('1970-01-01');
        } else {
          return Date.parse(dataGroup);
        }
      }).value();
      max = max === '0' || max === 'Overdue' ? lastMonday : Date.parse(max);
      if (max > lastMonday) {
        return max;
      } else {
        return lastMonday;
      }
    };

    Grouper.prototype.minPeriod = function() {
      return Date.today().monday().add(-7).days();
    };

    Grouper.prototype.addEmptyPeriods = function() {
      var dataGroups, max, min, period, results;
      min = this.minPeriod();
      max = this.maxPeriod().next().week();
      dataGroups = _(this.options).pluck('dataGroup');
      if (indexOf.call(dataGroups, '0') < 0) {
        this.options.push(this.emptyPeriod('0', true));
      }
      results = [];
      while (min <= max) {
        period = min.toString("yyyy-MM-dd");
        if (indexOf.call(dataGroups, period) < 0) {
          this.options.push(this.emptyPeriod(period, true));
        }
        results.push(min.next().week());
      }
      return results;
    };

    Grouper.prototype.addEmptyUsers = function() {
      var userIds, users;
      userIds = _(this.options).pluck('dataGroup');
      users = _(App.users).select(function(user) {
        var ref;
        return ref = user.login, indexOf.call(userIds, ref) < 0;
      });
      return _(users).each((function(_this) {
        return function(user) {
          return _this.options.push({
            story_ids: [],
            title: _this.title(user.login),
            dataGroup: user.login,
            sortID: _this.sortId(user.login),
            addButton: !_this.manager.isArchive()
          });
        };
      })(this));
    };

    Grouper.prototype.addEmptyStatuses = function() {
      var optionStatuses, statuses;
      optionStatuses = _(this.options).pluck('dataGroup');
      statuses = _(App.statuses).select(function(status) {
        return indexOf.call(optionStatuses, status) < 0;
      });
      return _(statuses).each((function(_this) {
        return function(status) {
          return _this.options.push({
            story_ids: [],
            title: _this.title(status),
            dataGroup: status,
            sortID: _this.sortId(status),
            addButton: false
          });
        };
      })(this));
    };

    Grouper.prototype.emptyPeriod = function(key, addButton) {
      if (addButton == null) {
        addButton = false;
      }
      return {
        story_ids: [],
        title: this.title(key),
        dataGroup: key,
        sortID: this.sortId(key),
        addButton: addButton
      };
    };

    Grouper.prototype.title = function(str) {
      switch (this.manager.groupedBy) {
        case 'periods':
          return this.convertPeriod(str);
        case 'status':
          return str.ucfirst();
        case 'tags':
          return this.convertTag(str).ucfirst();
        case 'users':
          return this.convertUser(str);
        default:
          return 'Ungrouped';
      }
    };

    Grouper.prototype.convertPeriod = function(str) {
      var current, end, end_month, end_year, start, start_month, start_year;
      if (str === '0') {
        return 'Unscheduled';
      }
      if (str === 'Overdue') {
        return 'Overdue';
      }
      start = Date.parse(str);
      end = new Date(start).add(6).days();
      current = new Date();
      start_month = end_month = start_year = end_year = "";
      if (start.getFullYear() === end.getFullYear()) {
        if (start.getFullYear() !== current.getFullYear()) {
          end_year = ", yyyy";
        }
        if (start.getMonth() === end.getMonth()) {
          start_month = "MMM";
        } else {
          start_month = end_month = "MMM";
        }
      } else {
        start_month = end_month = "MMM";
        start_year = end_year = ", yyyy";
      }
      return (start.toString(start_month + " d" + start_year)) + " - " + (end.toString(end_month + " d" + end_year));
    };

    Grouper.prototype.convertTag = function(str) {
      if (str === '') {
        return 'Without tags';
      } else {
        return _(App.tags).chain().detect(function(tag) {
          return tag === str;
        }).value();
      }
    };

    Grouper.prototype.convertUser = function(str) {
      var user;
      if (str === '') {
        return 'Without responsibilities';
      } else {
        user = _(App.users).detect(function(u) {
          return u.login === str.toString();
        });
        return "<img class='gravatar' src='" + user.gravatar_url + "' alt='" + user.name + "' title='" + user.name + "' data-active='" + user.active + "'> " + user.name;
      }
    };

    Grouper.prototype.sortId = function(key) {
      var tag, user;
      switch (this.manager.groupedBy) {
        case 'periods':
          if (key === '0') {
            return this.revertPeriod * Date.parse("+10year");
          }
          if (key === 'Overdue') {
            return this.revertPeriod * Date.parse("1970-01-01");
          }
          return this.revertPeriod * Date.parse(key);
        case 'tags':
          tag = _(App.tags).detect(function(tag) {
            return tag === key;
          });
          if (tag != null) {
            return tag.toLowerCase();
          } else {
            return '';
          }
          break;
        case 'users':
          user = _(App.users).detect(function(user) {
            return user.login === key;
          });
          if (user != null) {
            return user.name;
          } else {
            return '0';
          }
          break;
        case 'status':
          return -1 * _(App.statuses).indexOf(key.toLowerCase());
      }
    };

    return Grouper;

  })();

}).call(this);
(function() {
  var bind = function(fn, me){ return function(){ return fn.apply(me, arguments); }; },
    indexOf = [].indexOf || function(item) { for (var i = 0, l = this.length; i < l; i++) { if (i in this && this[i] === item) return i; } return -1; };

  App.Classes.Manager = (function() {
    function Manager() {
      this.isFiltered = bind(this.isFiltered, this);
      this.clearFilters = bind(this.clearFilters, this);
      this.destroyStory = bind(this.destroyStory, this);
      this.processStory = bind(this.processStory, this);
      this.updateStoryJSON = bind(this.updateStoryJSON, this);
      this.isArchive = bind(this.isArchive, this);
      this.getSavedRule = bind(this.getSavedRule, this);
      this.changeRule = bind(this.changeRule, this);
      this.applyFilter = bind(this.applyFilter, this);
      this.url = bind(this.url, this);
      this.params = bind(this.params, this);
      this.updateSidebar = bind(this.updateSidebar, this);
      this.regroup = bind(this.regroup, this);
      this.sort = bind(this.sort, this);
      this.filter = bind(this.filter, this);
      this.resetClearButtons = bind(this.resetClearButtons, this);
      this.getType = bind(this.getType, this);
      this.getSorting = bind(this.getSorting, this);
      this.getGrouping = bind(this.getGrouping, this);
      this.getStatuses = bind(this.getStatuses, this);
      this.getTags = bind(this.getTags, this);
      this.getUsers = bind(this.getUsers, this);
      this.getInfo = bind(this.getInfo, this);
      this.usersClearButton = $('#users_filter a.clear-filter');
      this.tagsClearButton = $('#tag_filter a.clear-filter');
      this.statusesClearButton = $('#status-filter a.clear-filter');
      this.getInfo();
      this.sorter = new App.Classes.Sorter(this);
      this.storyFilter = new App.Classes.StoryFilter(this);
      this.rules = new App.Classes.Rules(this);
      this.story = new App.Classes.Story;
      this.grouper = new App.Classes.Grouper(this);
      this.getSavedRule();
    }

    Manager.prototype.getInfo = function() {
      return this.getUsers().getTags().getStatuses().getGrouping().getSorting().getType();
    };

    Manager.prototype.getUsers = function() {
      this.users = $('#users_filter li.selected').map(function() {
        return $(this).data('login');
      }).get();
      return this;
    };

    Manager.prototype.getTags = function() {
      this.tags = $('#tag_filter li.selected').map(function() {
        return $(this).data('name').toString();
      }).get();
      return this;
    };

    Manager.prototype.getStatuses = function() {
      this.statuses = $('#status-filter li.selected').map(function() {
        return $(this).data('status');
      }).get();
      return this;
    };

    Manager.prototype.getGrouping = function() {
      this.groupedBy = $('#sidebar > .group > li.selected').data('type');
      return this;
    };

    Manager.prototype.getSorting = function() {
      this.sortedBy = 'manually';
      return this;
    };

    Manager.prototype.getType = function() {
      this.type = $("#filter-menu > li.active > a").data('type');
      return this;
    };

    Manager.prototype.resetClearButtons = function() {
      if (this.users.length) {
        this.usersClearButton.show();
      } else {
        this.usersClearButton.hide();
      }
      if (this.tags.length) {
        this.tagsClearButton.show();
      } else {
        this.tagsClearButton.hide();
      }
      if (this.statuses.length) {
        this.statusesClearButton.show();
      } else {
        this.statusesClearButton.hide();
      }
      return this;
    };

    Manager.prototype.filter = function(history) {
      if (history == null) {
        history = {
          pushState: false,
          replaceState: false
        };
      }
      this.storyFilter.setUsers(this.users).setTags(this.tags).setStatuses(this.statuses).filter();
      if (history.pushState) {
        App.history.push(this.url());
      }
      if (history.replaceState) {
        App.history.replace(this.url());
      }
      this.resetClearButtons();
      return this;
    };

    Manager.prototype.sort = function() {
      this.sorter.sort(this.sortedBy);
      return this;
    };

    Manager.prototype.regroup = function() {
      this.grouper.regroup();
      return this;
    };

    Manager.prototype.updateSidebar = function() {
      this.storyFilter.updateSidebar();
      return this;
    };

    Manager.prototype.params = function(short) {
      var obj;
      obj = {};
      if (this.users.length) {
        obj.users = this.users;
      }
      if (this.tags.length) {
        obj.tags = this.tags;
      }
      if (this.statuses.length) {
        obj.statuses = this.statuses;
      }
      if (!short) {
        obj.grouped_by = this.groupedBy;
      }
      obj.type = this.type;
      return obj;
    };

    Manager.prototype.url = function(short) {
      if (short == null) {
        short = false;
      }
      return "?" + decodeURIComponent($.param(this.params(short)));
    };

    Manager.prototype.applyFilter = function(json, history) {
      var oldGroupedBy, oldType, statuses, tags, users;
      if (history == null) {
        history = {
          pushState: false,
          replaceState: false
        };
      }
      oldType = this.type;
      oldGroupedBy = this.groupedBy;
      if (this.type !== json.type) {
        this.type = json.type || 'active';
        this.updateSidebar();
      }
      this.getSavedRule();
      this.users = users = _(json.users).map(function(el) {
        return parseInt(el);
      }) || [];
      this.tags = tags = _(json.tags).map(function(el) {
        return parseInt(el);
      }) || [];
      this.statuses = statuses = json.statuses || [];
      $('#tag_filter li').removeClass('selected');
      if (this.tags.length) {
        $('#tag_filter li').each(function() {
          if (_.include(tags, $(this).data('id'))) {
            return $(this).addClass('selected');
          }
        });
      }
      $('#users_filter li').removeClass('selected');
      if (this.users.length) {
        $('#users_filter li').each(function() {
          if (_.include(users, $(this).data('id'))) {
            return $(this).addClass('selected');
          }
        });
      }
      $('#status-filter li').removeClass('selected');
      if (this.statuses.length) {
        $('#status-filter li').each(function() {
          if (_.include(statuses, $(this).data('status'))) {
            return $(this).addClass('selected');
          }
        });
      }
      if (this.groupedBy !== json.grouped_by) {
        this.groupedBy = json.grouped_by || "periods";
        this.applyGrouping();
        $('#grouped_by ul li').removeClass('selected').find("a:contains(" + this.groupedBy + ")").parent().addClass('selected');
      }
      this.sortedBy = json.sorted_by || "manually";
      $('#sorted_by_params span').html("Sort by " + this.sortedBy);
      $('#sorted_by_params ul li').removeClass('selected').find("a:contains(" + this.sortedBy + ")").parent().addClass('selected');
      $("#filter-menu > li").removeClass('active').has("a[data-type=" + this.type + "]").addClass('active');
      if (oldType !== this.type || oldGroupedBy !== this.groupedBy) {
        this.regroup();
      }
      this.filter(history).sort();
      return this;
    };

    Manager.prototype.changeRule = function(history) {
      var rule;
      if (history == null) {
        history = {
          pushState: false,
          replaceState: false
        };
      }
      rule = this.getSavedRule();
      this.regroup().filter({
        pushState: true,
        replaceState: false
      }).sort();
      return this;
    };

    Manager.prototype.getSavedRule = function() {
      return this.currentRule = _(this.rules.json).detect((function(_this) {
        return function(rule) {
          if (_this.type !== rule.name) {
            return false;
          }
          if (_this.groupedBy !== rule.grouped_by) {
            return false;
          }
          return true;
        };
      })(this));
    };

    Manager.prototype.isArchive = function() {
      return this.type === 'archive';
    };

    Manager.prototype.updateStoryJSON = function(storyJSON) {
      var story;
      story = _(App.stories).detect(function(story) {
        return story.id === storyJSON.id;
      });
      if (story) {
        if (this.isStoryEqual(storyJSON, story)) {
          return false;
        }
        return story = $.extend(story, storyJSON);
      } else {
        return App.stories.push(storyJSON);
      }
    };

    Manager.prototype.processStory = function(storyJSON, faye) {
      var $group, $story, add, storyHtml, visible;
      if (faye == null) {
        faye = false;
      }
      if (!this.updateStoryJSON(storyJSON)) {
        return false;
      }
      storyHtml = this.story.setStory(storyJSON.id).html();
      switch (this.groupedBy) {
        case 'periods':
          $group = $(".story-list ul[data-group=" + storyJSON.period + "]");
          break;
        case 'status':
          $group = $(".story-list ul[data-group='" + storyJSON.status + "']");
          break;
        case 'tags':
          if (storyJSON.tags.length) {
            $group = $(".story-list ul").filter(function() {
              var ref;
              return ref = $(this).data('group'), indexOf.call(storyJSON.tags, ref) >= 0;
            });
          } else {
            $group = $('.story-list ul [data-id=0]');
          }
          break;
        case 'users':
          if (storyJSON.users.length) {
            $group = $(".story-list ul").filter(function() {
              var ref;
              return ref = $(this).data('group'), indexOf.call(storyJSON.users, ref) >= 0;
            });
          } else {
            $group = $('.story-list ul [data-id=0]');
          }
          break;
        default:
          $group = $('.story-list ul');
      }
      if ($group.length) {
        $('.story-list').find("li[data-id=" + storyJSON.id + "]").remove();
        if (faye) {
          $story = $(storyHtml);
          visible = this.storyFilter.isStoryVisible(storyJSON);
          if (!visible) {
            $story.hide();
          }
          if ((add = $group.find('.add')).length) {
            add.before($story.clone());
          } else {
            $group.append($story.clone());
          }
          if (visible) {
            this.sorter.sort(this.sortedBy, $group);
          }
        } else {
          if ((add = $group.find('.add')).length) {
            add.before(storyHtml);
          } else {
            $group.append(storyHtml);
          }
        }
      } else {
        this.regroup().filter().sort();
      }
      return $("li[data-id=" + storyJSON.id + "]:visible").effect('pulsate', {
        times: 3
      }, 600);
    };

    Manager.prototype.destroyStory = function(story_id) {
      App.stories = _(App.stories).reject(function(story) {
        return story.id === !story_id;
      });
      return $('.story-list').find("li[data-id=" + story_id + "]").remove();
    };

    Manager.prototype.clearFilters = function() {
      this.users = this.tags = this.statuses = [];
      $('#tag_filter li').removeClass('selected');
      $('#users_filter li').removeClass('selected');
      $('#status-filter li').removeClass('selected');
      return this;
    };

    Manager.prototype.isFiltered = function() {
      return this.users.length !== 0 || this.tags.length !== 0 || this.statuses.length !== 0;
    };

    Manager.prototype.isStoryEqual = function(a, b) {
      var checkEqualKeys, i, key, len, result;
      checkEqualKeys = ['id', 'status', 'tag_ids', 'user_ids', 'actual_estimate_time', 'actual_time', 'estimate_time', 'name', 'period', 'archive', 'with_questions'];
      result = true;
      for (i = 0, len = checkEqualKeys.length; i < len; i++) {
        key = checkEqualKeys[i];
        if (!_.isEqual(a[key], b[key])) {
          result = false;
        }
      }
      return result;
    };

    return Manager;

  })();

}).call(this);
(function() {
  var bind = function(fn, me){ return function(){ return fn.apply(me, arguments); }; };

  App.Classes.StoryMover = (function() {
    function StoryMover() {
      this.sendRequest = bind(this.sendRequest, this);
      this.processStoryAttributes = bind(this.processStoryAttributes, this);
      this.removeDuplicatesFromTargetList = bind(this.removeDuplicatesFromTargetList, this);
      this.move = bind(this.move, this);
      this.setOptions = bind(this.setOptions, this);
      this.story = new App.Classes.Story;
    }

    StoryMover.prototype.setOptions = function(options) {
      this.options = options;
      return this;
    };

    StoryMover.prototype.move = function() {
      var storyHtml;
      this.story.setStory(this.options.story.data('id'));
      if (this.options.copy) {
        this.removeDuplicatesFromTargetList();
        this.processStoryAttributes();
        storyHtml = this.story.html();
        $(".story-list > ul > li[data-id=" + (this.options.story.data('id')) + "]").replaceWith(storyHtml);
        if (this.options.positions.start === 0) {
          $(this.options.lists.start).prepend(storyHtml);
        } else {
          $(this.options.lists.start).children('li').eq(this.options.positions.start - 1).after(storyHtml);
        }
      } else {
        if (this.options.lists.changed) {
          if ($(this.options.lists.end).data('group') === 0) {
            $(".story-list ul li[data-id=" + ($(this.options.story).data('id')) + "]:not(:last)").remove();
          }
          this.removeDuplicatesFromTargetList();
          this.processStoryAttributes();
          $(".story-list > ul > li[data-id=" + (this.options.story.data('id')) + "]").replaceWith(this.story.html());
        }
      }
      if (this.options.lists.changed) {
        $(this.options.lists.start).add(this.options.lists.end).calculate();
      }
      this.story.checkForArchive();
      App.manager.updateSidebar();
      return this.sendRequest();
    };

    StoryMover.prototype.removeDuplicatesFromTargetList = function() {
      return $(this.options.lists.end).children("li[data-id=" + (this.options.story.data('id')) + "]").not(this.options.story).remove();
    };

    StoryMover.prototype.processStoryAttributes = function() {
      var newer, older;
      older = $(this.options.lists.start).data('group');
      newer = $(this.options.lists.end).data('group');
      switch (App.manager.groupedBy) {
        case 'tags':
          if (this.options.copy) {
            return this.story.addTag(newer);
          } else {
            return this.story.replaceTag(older, newer);
          }
          break;
        case 'users':
          if (this.options.copy) {
            return this.story.addUser(newer);
          } else {
            return this.story.replaceUser(older, newer);
          }
          break;
        case 'status':
          return this.story.changeStatus(newer);
        case 'periods':
          return this.story.changePeriod(newer);
      }
    };

    StoryMover.prototype.sendRequest = function() {
      var model;
      if (App.manager.sortedBy === 'manually' || this.options.lists.changed) {
        model = new App.Classes.StoriesModel(this.options);
        return model.send();
      }
    };

    return StoryMover;

  })();

}).call(this);
(function() {
  var bind = function(fn, me){ return function(){ return fn.apply(me, arguments); }; };

  App.Classes.Rules = (function() {
    function Rules(manager) {
      this.manager = manager;
      this.orderModify = bind(this.orderModify, this);
      this.modify = bind(this.modify, this);
      this.getManualOrder = bind(this.getManualOrder, this);
      this.find = bind(this.find, this);
      this.replace = bind(this.replace, this);
      this.remove = bind(this.remove, this);
      this.add = bind(this.add, this);
      this.save = bind(this.save, this);
      this.getRules = bind(this.getRules, this);
      this.json = App.rules;
      this["default"] = $('#filter-menu li.active a').data('id');
    }

    Rules.prototype.getRules = function() {
      return $.ajax({
        url: $('#rules_json_path').val(),
        dataType: 'json',
        success: (function(_this) {
          return function(json) {
            _this.json = json;
          };
        })(this)
      });
    };

    Rules.prototype.save = function(name) {
      var params;
      params = this.manager.params();
      params.filter_name = name;
      params.manually = this.getManualOrder();
      return $.ajax({
        url: $('#filter_save_path').val(),
        dataType: 'script',
        type: 'post',
        data: params
      });
    };

    Rules.prototype.add = function(rule) {
      this.json.push(rule);
      return this;
    };

    Rules.prototype.remove = function(id) {
      this.json = _(this.json).reject(function(rule) {
        return rule.id === id;
      });
      return this;
    };

    Rules.prototype.replace = function(rule) {
      this.remove(rule.id).add(rule);
      return this;
    };

    Rules.prototype.find = function(id) {
      var rule;
      return rule = _(this.json).detect(function(el) {
        return el.id === id;
      });
    };

    Rules.prototype.getManualOrder = function() {
      var manually;
      manually = [];
      $('.story-list ul').each(function() {
        var order;
        order = {};
        order.group_name = $(this).data('group');
        order.story_ids = $(this).children().map(function() {
          return $(this).data('id');
        }).get();
        return manually.push(order);
      });
      return manually;
    };

    Rules.prototype.modify = function(data) {
      var rule;
      rule = this.find(data.rule_id);
      this.orderModify(rule, {
        group: data.to,
        ids: data.to_ids
      });
      if (data.from === !data.to) {
        return this.orderModify(rule, {
          group: data.from,
          ids: data.from_ids
        });
      }
    };

    Rules.prototype.orderModify = function(rule, data) {
      var manualOrder;
      data.group = data.group.toString();
      manualOrder = _(rule.manually).detect(function(el) {
        if (rule) {
          return el.group_name === data.group;
        }
      });
      if (manualOrder) {
        manualOrder.story_ids = data.ids;
        rule.manually = _(rule.manually).reject(function(order) {
          return order.group_name === manualOrder.group_name;
        });
        rule.manually.push(manualOrder);
      } else {
        rule.manually.push({
          group_name: data.group,
          story_ids: data.ids
        });
      }
      return this.replace(rule);
    };

    return Rules;

  })();

}).call(this);
(function() {
  var bind = function(fn, me){ return function(){ return fn.apply(me, arguments); }; };

  App.Classes.Sorter = (function() {
    function Sorter(manager) {
      this.manager = manager;
      this.manuallComparator = bind(this.manuallComparator, this);
      this.sortManually = bind(this.sortManually, this);
      this.resetAddButtons = bind(this.resetAddButtons, this);
    }

    Sorter.prototype.sort = function(type, ul1) {
      this.ul = ul1 != null ? ul1 : null;
      if (!this.ul) {
        this.ul = $('.story-list ul');
      }
      if (type === "manually" && this.manager.currentRule) {
        this.sortManually();
      } else {
        this.ul.each(function() {
          var plus;
          plus = $(this).children('.add').detach();
          $(this).children().tsort({
            order: 'asc',
            attr: "data-" + type
          });
          return $(this).append(plus);
        });
      }
      return this.resetAddButtons();
    };

    Sorter.prototype.resetAddButtons = function() {
      return this.ul.find('li.add').each(function() {
        return $(this).parent().append($(this));
      });
    };

    Sorter.prototype.sortManually = function() {
      var rule;
      rule = this.manager.currentRule;
      if (rule.manually.length !== 0) {
        return this.ul.each((function(_this) {
          return function(index, ul) {
            var $ul, group, groupOrder;
            $ul = $(ul);
            group = $ul.data("group").toString();
            groupOrder = _(rule.manually).detect(function(rule) {
              return rule.group_name === group;
            });
            if (groupOrder != null) {
              _this.order = groupOrder.story_ids;
              _this.orderLength = _this.order.length;
              return $ul.children('li').tsort({
                sortFunction: _this.manuallComparator
              });
            }
          };
        })(this));
      }
    };

    Sorter.prototype.manuallComparator = function(a, b) {
      var aIndex, bIndex;
      aIndex = _.indexOf(this.order, a.e.data('id'));
      bIndex = _.indexOf(this.order, b.e.data('id'));
      if (aIndex === -1) {
        aIndex = ++this.orderLength;
      }
      if (bIndex === -1) {
        bIndex = ++this.orderLength;
      }
      if (aIndex === bIndex) {
        return 0;
      }
      if (aIndex > bIndex) {
        return 1;
      }
      if (aIndex < bIndex) {
        return -1;
      }
    };

    return Sorter;

  })();

}).call(this);
(function() {
  var bind = function(fn, me){ return function(){ return fn.apply(me, arguments); }; };

  App.Classes.Story = (function() {
    function Story() {
      this.checkForArchive = bind(this.checkForArchive, this);
      this.statusIndex = bind(this.statusIndex, this);
      this.changePeriod = bind(this.changePeriod, this);
      this.changeStatus = bind(this.changeStatus, this);
      this.replaceUser = bind(this.replaceUser, this);
      this.addUser = bind(this.addUser, this);
      this.replaceTag = bind(this.replaceTag, this);
      this.addTag = bind(this.addTag, this);
      this.setStory = bind(this.setStory, this);
      this.html = bind(this.html, this);
      this.json = bind(this.json, this);
      this.usersJSON = App.users;
      this.tagsJSON = App.tags;
      this.statuses = App.statuses;
      this.project = App.project;
    }

    Story.prototype.json = function() {
      return {
        id: this.storyJSON.id,
        status: this.storyJSON.status,
        date: this.storyJSON.created_at,
        statusIndex: this.statusIndex(),
        link: "/projects/" + this.project + "/stories/" + this.storyJSON.id,
        name: this.storyJSON.name,
        tags: this.storyJSON.tags,
        participants: this.storyJSON.users,
        actualEtimate: this.storyJSON.actual_estimate_time,
        withQuestions: this.storyJSON.with_questions
      };
    };

    Story.prototype.html = function() {
      return window.JST['story'](this.json());
    };

    Story.prototype.setStory = function(id) {
      this.storyJSON = _(App.stories).detect(function(story) {
        return story.id === id;
      });
      return this;
    };

    Story.prototype.addTag = function(tagName) {
      this.storyJSON.tags.push(tagName);
      return this;
    };

    Story.prototype.replaceTag = function(oldTagName, newTagName) {
      if (newTagName === '') {
        this.storyJSON.tags = [];
      } else {
        this.storyJSON.tags = _(this.storyJSON.tags).chain().without(oldTagName).push(newTagName).value();
      }
      return this;
    };

    Story.prototype.addUser = function(userLogin) {
      this.storyJSON.users.push(userLogin);
      return this;
    };

    Story.prototype.replaceUser = function(oldUserLogin, newUserLogin) {
      if (newUserLogin === 0) {
        this.storyJSON.users = [];
      } else {
        this.storyJSON.users = _(this.storyJSON.users).chain().without(oldUserLogin).push(newUserLogin).value();
      }
      return this;
    };

    Story.prototype.changeStatus = function(status) {
      this.storyJSON.status = status;
      return this;
    };

    Story.prototype.changePeriod = function(period) {
      this.storyJSON.period = period;
      return this;
    };

    Story.prototype.statusIndex = function() {
      return _(this.statuses).indexOf(this.storyJSON.status);
    };

    Story.prototype.checkForArchive = function() {
      var currentPeriod, mondayTwoWeeksAgo, periodStart;
      switch (this.storyJSON.status) {
        case 'accepted':
          if (this.storyJSON.period !== 0) {
            periodStart = Date.parse(this.storyJSON.period);
            currentPeriod = Date.today().monday().addWeeks(-1);
            if (periodStart < currentPeriod) {
              return this.storyJSON.archive = 1;
            }
          }
          break;
        case 'completed':
          if (this.storyJSON.period !== 0) {
            periodStart = Date.parse(this.storyJSON.period);
            mondayTwoWeeksAgo = Date.today().monday().addWeeks(-3);
            if (periodStart < mondayTwoWeeksAgo) {
              return this.storyJSON.archive = 1;
            }
          }
      }
    };

    return Story;

  })();

}).call(this);
(function() {
  var bind = function(fn, me){ return function(){ return fn.apply(me, arguments); }; };

  App.Classes.StoryCreator = (function() {
    function StoryCreator() {
      this.validateTime = bind(this.validateTime, this);
      this.validateTitle = bind(this.validateTitle, this);
      this.resetValues = bind(this.resetValues, this);
      this.setValues = bind(this.setValues, this);
      this.getValues = bind(this.getValues, this);
      this.setPosition = bind(this.setPosition, this);
      this.bindings = bind(this.bindings, this);
      this.popupHtml = bind(this.popupHtml, this);
      this.template = JST['add-story'];
      this.types = {
        periods: "period_start",
        status: "status",
        tags: "tag_list",
        users: "performer_logins",
        ungrouped: 'type'
      };
      this.resetValues();
      this.bindings();
    }

    StoryCreator.prototype.popupHtml = function(selector) {
      var groupBy, id, options;
      options = {};
      options.story = {};
      options.showTimePicker = true;
      groupBy = App.manager.groupedBy;
      this.currentGroup = selector.parents(".stories-period");
      id = this.currentGroup.find("ul.ui-sortable").data('group');
      if (groupBy === "statuses" && id === "to estimate") {
        options.showTimePicker = false;
      }
      options.story = {
        type: this.types[groupBy],
        value: id
      };
      return this.template(options);
    };

    StoryCreator.prototype.bindings = function() {
      return $('.add > a').live('click', (function(_this) {
        return function(e) {
          _this.currentSelector = $(e.target);
          if (!_this.currentSelector.data('used')) {
            _this.currentSelector.menuPopup({
              selector: _this.currentSelector.next(),
              show: function(popup) {
                var $selector, offsetBottom;
                $selector = $(e.target);
                offsetBottom = $(document).height() - $selector.offset().top - $selector.outerHeight();
                popup.html(_this.popupHtml($selector)).find('#story_name').focus();
                _this.setPosition($selector, offsetBottom);
                _this.setValues(popup);
                return $('#new-story-special', popup).bind('submit', function() {
                  var $input, $title;
                  $title = popup.find('#story_name');
                  $input = popup.find('#story_estimate');
                  if (!_this.validateTitle($title.val())) {
                    $title.addClass('error');
                    $title.one('keypress', function() {
                      return $(this).removeClass('error');
                    }).focus();
                    $.publish('tracker:flash:invoke', ['error', "Name of the story can't be blank"]);
                    return false;
                  }
                  if ($input.val() && !_this.validateTime($input.val())) {
                    $input.addClass('error');
                    $.publish('tracker:flash:invoke', ['error', "Wrong estimate"]);
                    return false;
                  }
                  return popup.trigger('menupopup:hide');
                });
              },
              hide: function(popup) {
                _this.getValues(popup);
                return popup.html('');
              }
            });
            _this.currentSelector.trigger('menupopup:show');
            _this.currentSelector.data('used', true);
          }
          return false;
        };
      })(this));
    };

    StoryCreator.prototype.setPosition = function(selector, position) {
      var $popup, margin, outer;
      $popup = selector.next().find(".new-story-block");
      outer = $popup.outerHeight() - 110 - selector.outerHeight() / 2;
      if ((margin = outer - position) < 0) {
        margin = 0;
      }
      return $popup.css("top", "-" + (82 + margin) + "px").find(".corner").css("top", (110 + margin) + "px");
    };

    StoryCreator.prototype.getValues = function(popup) {
      this.title = popup.find('#story_name').val();
      return this.description = popup.find('#story_description').val();
    };

    StoryCreator.prototype.setValues = function(popup) {
      popup.find('#story_name').val(this.title);
      popup.find('#story_description').val(this.description);
      return App.slider.init(popup.find('.slider'), popup.find('#story_estimate'), {
        value: this.value
      }).setFormatedTime(this.value);
    };

    StoryCreator.prototype.resetValues = function() {
      this.value = 0;
      return this.description = this.title = '';
    };

    StoryCreator.prototype.validateTitle = function(title) {
      title = $.trim(title);
      return $.trim(/^(?:\[.*\])?(.*)/.exec(title)[1]).length > 0;
    };

    StoryCreator.prototype.validateTime = function(time) {
      return App.slider.validateTime(time);
    };

    return StoryCreator;

  })();

}).call(this);
(function() {
  var bind = function(fn, me){ return function(){ return fn.apply(me, arguments); }; },
    indexOf = [].indexOf || function(item) { for (var i = 0, l = this.length; i < l; i++) { if (i in this && this[i] === item) return i; } return -1; };

  App.Classes.Tag = (function() {
    function Tag(id1) {
      this.id = id1;
      this.name = bind(this.name, this);
      this.find = bind(this.find, this);
      this.id = Number(this.id);
      if (this.id) {
        this.find(this.id);
      }
      this.exists = this.tag ? true : false;
    }

    Tag.prototype.find = function(id) {
      return this.tag = _(App.tags).detect(function(tag) {
        return tag.id.toString() === id.toString();
      });
    };

    Tag.prototype.name = function() {
      if (this.exists) {
        return this.tag.name;
      }
    };

    return Tag;

  })();

  App.Classes.Tag.update = function(tags) {
    var ids;
    ids = _(tags).pluck('id');
    return App.tags = _(App.tags).chain().reject(function(el) {
      var ref;
      return ref = el.id, indexOf.call(ids, ref) >= 0;
    }).union(tags).value();
  };

}).call(this);
(function() {
  $(function() {
    return $('.transactions').detailsLink();
  });

}).call(this);
(function() {
  var bind = function(fn, me){ return function(){ return fn.apply(me, arguments); }; };

  App.Classes.User = (function() {
    function User(id1) {
      this.id = id1;
      this.name = bind(this.name, this);
      this.login = bind(this.login, this);
      this.gravatar = bind(this.gravatar, this);
      this.find = bind(this.find, this);
      this.id = Number(this.id);
      if (this.id) {
        this.find(this.id);
      }
      this.exists = this.user ? true : false;
    }

    User.prototype.find = function(id) {
      return this.user = _(App.users).detect(function(user) {
        return user.id === id;
      });
    };

    User.prototype.gravatar = function(size) {
      if (size == null) {
        size = 18;
      }
      return "https://secure.gravatar.com/avatar/" + this.user.email_md5 + "?size=" + size + "&d=retro";
    };

    User.prototype.login = function() {
      if (this.exists) {
        return this.user.login;
      }
    };

    User.prototype.name = function() {
      if (this.exists) {
        return this.user.name;
      }
    };

    return User;

  })();

}).call(this);
(function() {
  App.helpers = {};

  App.helpers.velocityFor = function(period) {
    var velocityObj;
    velocityObj = _(App.velocities).detect(function(velocity) {
      return velocity.period === period;
    });
    if (!velocityObj) {
      velocityObj = _(App.velocities).detect(function(velocity) {
        return velocity.period === 'default';
      });
    }
    return velocityObj.velocity;
  };

  App.helpers.scrollToNewStory = function() {
    var $scrollObj, $story, id, positionToScroll;
    if (id = $.cookie('new_story')) {
      $story = $("#planned-stories li[data-id=" + id + "]:first");
      if (!$story.length) {
        return true;
      }
      positionToScroll = $story.offset().top + $story.outerHeight() - $(window).height() + 15;
      if (positionToScroll > 0) {
        $scrollObj = $.browser.webkit ? $("body") : $("html");
        $scrollObj.animate({
          scrollTop: positionToScroll
        }, 1000, function() {
          return $story.effect('pulsate', {
            times: 3
          }, 600);
        });
      } else {
        $story.effect('pulsate', {
          times: 3
        }, 600);
      }
      return $.cookie('new_story', null);
    }
  };

  App.helpers.initSpoilers = function() {
    var forwardAttribute, spoiler;
    forwardAttribute = function(fromObject, attrName) {
      var value;
      value = $(fromObject).attr(attrName);
      if (value) {
        return attrName + "='" + value + "'";
      } else {
        return '';
      }
    };
    spoiler = $('cut');
    if (spoiler.length) {
      spoiler.each(function() {
        var content, spoilerHtml, title;
        title = $(this).attr('text') || 'Read more...';
        content = $(this).html();
        spoilerHtml = " <div class='spoiler' " + (forwardAttribute(this, 'id')) + "> <div class='cut-title'>" + title + "</div> <div class='cut-content'>" + content + "</div> </div>";
        return $(this).replaceWith(spoilerHtml);
      });
      return $('.spoiler').on('click', '.cut-title', function() {
        return $(this).next().toggle();
      });
    }
  };

  App.helpers.changeProject = function(project, parentWindow) {
    var selectedMenu, url;
    if (parentWindow == null) {
      parentWindow = false;
    }
    if ((selectedMenu = $('menu').find('a.active')).length) {
      url = selectedMenu.attr('href').replace(/projects\/\d+/, "projects/" + project);
      if (parentWindow) {
        window.open(url);
      } else {
        document.location.href = url;
      }
      return false;
    }
  };

}).call(this);
(function() {
  var postForm;

  postForm = function(form) {
    var jForm;
    jForm = $(form);
    return $.post(jForm.attr("action"), jForm.serialize());
  };

  $(function() {
    return $("#invoicers_form input[type='checkbox']").change(function() {
      return postForm("#invoicers_form");
    });
  });

}).call(this);
(function() {
  $(function() {
    App.helpers.initSpoilers();
    return App.fileUploader = new App.Classes.FileUploader();
  });

}).call(this);
(function() {
  var EstimateHistory, Responsibility, Tags;

  Tags = {
    init: function() {
      this.tagList = $('.tags-list').not('.search-block .tags-list');
      this.hiddenField = $('#story_tag_list');
      this.newTagForm = $('#add_new_tag #new_tag');
      this.newTagShowSpan = $('#show_new_tag_form');
      if (!Project.readonly) {
        return this.bind();
      }
    },
    bind: function() {
      var that;
      that = this;
      this.tagList.on('click', 'li', function() {
        $(this).toggleClass('selected');
        if (that.isShowPage()) {
          return that.changeTags($(this));
        }
      });
      $(document).on('click', function(e) {
        if (!$(e.target).closest('#add_new_tag').length) {
          return that.hideAddTagForm();
        }
      });
      this.newTagShowSpan.bind('click', $.proxy(this.showAddTagForm, this));
      this.newTagForm.bind('keydown', 'return', function() {
        if ($.trim($(this).val()).length !== 0) {
          Tags.addTags();
        }
        return false;
      });
      this.newTagForm.bind('keydown', 'esc', $.proxy(this.hideAddTagForm, this));
      return $(this.newTagForm).ajaxSuccess(function() {
        return $(document).trigger('change:status');
      });
    },
    changeTags: function($li) {
      return $.ajax({
        url: (window.location.href.replace(/edit/, '')) + "/tag",
        type: 'put',
        data: "story[tag_list]=" + ($('.tags-list').find('.selected').map(function() {
          return $(this).text();
        }).get().join(',')),
        beforeSend: function() {
          if ($li) {
            return $li.fadeTo('fast', 0.5).find('.wait').show();
          }
        },
        success: function() {
          if ($li) {
            return $li.fadeTo('fast', 1).find('.wait').hide();
          }
        }
      });
    },
    existedTags: function() {
      return this.tagList.find('li').map(function() {
        return $.trim($(this).text().toLowerCase());
      }).get();
    },
    updateHiddenInput: function() {
      return this.hiddenField.val($('.tags-list').find('.selected').map(function() {
        return $(this).text();
      }).get().join(','));
    },
    showAddTagForm: function() {
      this.newTagShowSpan.hide();
      return this.newTagForm.show().focus();
    },
    addTags: function() {
      var $tagList, existed, tags, that;
      that = this;
      existed = this.existedTags();
      tags = this.newTagForm.val().split(',');
      $tagList = this.tagList;
      return $.each(tags, function(i, val) {
        var span, tag;
        tag = $.trim(val).toLowerCase();
        if (tag.length !== 0) {
          if ($.inArray(tag, existed) === -1) {
            existed.push(tag);
            $("<li class='selected'><div class='wait'></div><span>" + tag + "</span></li>").appendTo($tagList.find('ul'));
          } else {
            span = $("span").filter(function() {
              return $.trim(this.innerHTML.toLowerCase()) === tag;
            });
            span.closest('li').addClass('selected');
          }
          that.hideAddTagForm();
          if ($('#new_story')[0] == null) {
            return that.changeTags();
          }
        }
      });
    },
    hideAddTagForm: function() {
      this.newTagShowSpan.show();
      return this.newTagForm.hide().val('');
    },
    isShowPage: function() {
      return !$('.editForm').length;
    }
  };

  Responsibility = {
    init: function() {
      this.responsibilityList = $('.users-list');
      this.hiddenField = $('#story_performer_logins');
      if (!Project.readonly) {
        return this.bind();
      }
    },
    bind: function() {
      var that;
      that = this;
      return this.responsibilityList.find('li').bind('click', function() {
        $(this).toggleClass('selected');
        if (that.isShowPage()) {
          return that.changeResponsibility($(this));
        }
      });
    },
    changeResponsibility: function($li) {
      $.ajax({
        url: (window.location.href.replace(/\/edit/, '')) + "/performer",
        type: 'put',
        data: "story[performer_logins]=" + ($('.users-list').find('.selected').map(function() {
          return $(this).data('login');
        }).get().join(',')),
        beforeSend: function() {
          return $li.find('.gravatar').fadeTo('fast', 0.5).end().find('.wait').show();
        },
        success: function() {
          return $li.find('.wait').hide().end().find('.gravatar').fadeTo('fast', 1);
        }
      });
      if ($li.hasClass('deactivated')) {
        return $li.unbind('click');
      }
    },
    updateHiddenInput: function() {
      return this.hiddenField.val(this.responsibilityList.find('.selected').map(function() {
        return $(this).data('login');
      }).get().join(','));
    },
    isShowPage: function() {
      return !$('.editForm').length;
    }
  };

  App.Estimate = {
    init: function() {
      this.estimateBlock = $('.estimated-time .value');
      this.estimateTime = this.estimateBlock.find('span#time');
      this.input = this.estimateBlock.find('#estimate');
      if (!Project.readonly) {
        return this.bind();
      }
    },
    bind: function() {
      var that;
      that = this;
      $('.status-info').on('click', '.value', function() {
        return that.showEdit();
      });
      $(document).find("*").bind('click', function(e) {
        if ($(e.target).closest('.estimated-time').length === 0) {
          return that.hideEdit();
        }
      });
      $('.status-info').on('keypress', '#estimate', function() {
        return that.hideError();
      });
      $('.status-info').on('keydown', '#estimate', 'esc', function() {
        return that.hideEdit();
      });
      $('.status-info').on('keydown', '#estimate', 'tab', function() {
        return that.hideEdit();
      });
      return $('#estimate-list').on('change', this.reinit());
    },
    fillLastChanger: function(user) {
      if (user) {
        return $('.last-changer').text("Last estimate was changed by " + user);
      } else {
        return $('.last-changer').text("");
      }
    },
    showEdit: function() {
      this.reinit();
      this.estimateTime.hide();
      return this.input.addClass('active').focus();
    },
    hideEdit: function() {
      this.reinit();
      this.estimateTime.show();
      this.input.removeClass('active');
      return this.hideError();
    },
    showError: function() {
      return this.input.addClass('error');
    },
    hideError: function() {
      return this.input.removeClass('error');
    },
    reinit: function() {
      this.estimateBlock = $('.estimated-time .value');
      this.estimateTime = this.estimateBlock.find('span#time');
      return this.input = this.estimateBlock.find('#estimate');
    }
  };

  EstimateHistory = {
    init: function() {
      if (location.href.match(/stories\/\d+\/?$/) && !location.href.match(/stories\/new/) && !location.href.match(/stories\/\d\/edit/)) {
        this.historyBlock = $('.estimate-history');
        this.estimateData = {};
        return this.bind();
      }
    },
    bind: function() {
      var that;
      that = this;
      $('.status-info').on('click', '.hide-info', function() {
        return that.hideEstimates();
      });
      return $('.status-info').on('click', '.more-info', function() {
        return that.showEstimates();
      });
    },
    showEstimates: function() {
      return $('.estimate-history').slideUp('slow', function() {
        $('.more-info').hide();
        $('.last-changer').hide();
        $('#estimate-list').show();
        $('.hide-info').show();
        return $('.estimate-history').slideDown('slow');
      });
    },
    hideEstimates: function() {
      return $('.estimate-history').slideUp('slow', function() {
        $('.hide-info').hide();
        $('#estimate-list').hide();
        $('.last-changer').hide();
        $('.last-changer').css('display', 'inline');
        $('.more-info').show();
        return $('.estimate-history').slideDown('slow');
      });
    }
  };

  $(function() {
    $(document).bind('change:status', function() {
      if ($('.possible-status').find('.status').size() === 0) {
        return $('.possible-status').hide();
      }
    });
    $(document).trigger('change:status');
    Tags.init();
    Responsibility.init();
    EstimateHistory.init();
    App.Estimate.init();
    $('form.edit_story, form.new_story').live('submit', function() {
      Tags.updateHiddenInput();
      return Responsibility.updateHiddenInput();
    });
    return $('.possible-status .edit_story input[type="submit"]').live('click', function() {
      return $('#story_status').val($(this).data('status'));
    });
  });

}).call(this);
(function() {
  var highLightSelectedPeriod, initLinks, initPeriodTabs;

  window.initStatisticTabs = function(selector) {
    selector || (selector = "ul.tabs:visible");
    return $(selector).each(function() {
      return $(this).tabs($(this).parent().find(".panes:first > .pane"), {
        effect: 'fade'
      }).history();
    });
  };

  initPeriodTabs = function() {
    var anchor, currentLink, params;
    anchor = window.location.hash.split("#")[1];
    $("#iteration_tabs").tabs($(".periodsAndStories"), {
      initialIndex: 0
    });
    if (anchor) {
      params = anchor.split('-');
      currentLink = $("#iteration_tabs a[href*=" + params[0] + "]");
      if (currentLink.length) {
        $('.periodsAndStories').css('display', 'none');
        return currentLink.trigger('click');
      }
    }
  };

  initLinks = function() {
    return $('#iteration_tabs a').bind('click', function() {
      var period_id;
      period_id = $(this).attr('href').match(/\d+$/);
      $(".periodsAndStories").hide();
      $("#periodsAndStories" + period_id).show();
      window.location.hash = "";
      if ($("#periodsAndStories" + period_id + " > .loading").length === 0) {
        window.location.hash = period_id + "-work_time";
        return false;
      }
    });
  };

  window.chooseTabAndMakeAnchor = function(period_id) {
    var anchor, hsh, strTime;
    hsh = {
      work_time: 0,
      ideal_time: 1,
      velocity: 2
    };
    anchor = window.location.hash.split("#");
    strTime = "work_time";
    if (anchor[1] !== void 0) {
      strTime = anchor[1].split("-")[1];
    }
    return window.location.hash = period_id + "-" + strTime;
  };

  highLightSelectedPeriod = function() {
    var currentLink;
    if (window.location.pathname.match(/periods\/\d+$/)) {
      currentLink = window.location.pathname.match(/\d+$/);
      if (currentLink.length) {
        return $("a[href='" + window.location.pathname + "']").addClass('current');
      }
    }
  };

  $(function() {
    if (!window.location.pathname.match(/periods\/\d+$/)) {
      initLinks();
    }
    if (!window.location.pathname.match(/periods\/\d+$/)) {
      initPeriodTabs();
    }
    window.initStatisticTabs("#main > .periodsAndStories:not(:has(.loading)) ul.tabs");
    return highLightSelectedPeriod();
  });

}).call(this);
(function() {
  var TaskTransfer;

  TaskTransfer = {
    init: function() {
      if (!$('#task_transfer_form').length) {
        return;
      }
      this.$_allStories = $('.all-stories-select');
      this.$_notCompletedStories = $('.not-completed-stories-select');
      this.$_switch = $('#forms_task_transfer_show_all_stories');
      this.$_label = $('#task_transfer_story_id_label');
      this.initSelector();
      return this.bind();
    },
    initSelector: function() {
      return this.showNotCompleted();
    },
    bind: function() {
      return this.$_switch.on('change', (function(_this) {
        return function() {
          if (_this.$_switch.is(':checked')) {
            return _this.showAll();
          } else {
            return _this.showNotCompleted();
          }
        };
      })(this));
    },
    showAll: function() {
      this.$_allStories.show();
      this.$_notCompletedStories.hide();
      this.$_allStories.prop('disabled', false);
      this.$_notCompletedStories.prop('disabled', true);
      this.$_label.prop('for', this.$_allStories.attr('id'));
      return this.syncSelectedValue(this.$_notCompletedStories, this.$_allStories);
    },
    showNotCompleted: function() {
      this.$_allStories.hide();
      this.$_notCompletedStories.show();
      this.$_allStories.prop('disabled', true);
      this.$_notCompletedStories.prop('disabled', false);
      this.$_label.prop('for', this.$_notCompletedStories.attr('id'));
      return this.syncSelectedValue(this.$_allStories, this.$_notCompletedStories);
    },
    syncSelectedValue: function(from, to) {
      var value;
      value = from.find(":selected").prop("value");
      to.find(":selected").removeProp("selected");
      return to.find("option[value=" + value + "]").prop("selected", "selected");
    }
  };

  $(function() {
    return TaskTransfer.init();
  });

}).call(this);
(function() {
  var Tasks;

  Tasks = {
    init: function() {
      if (!Project.readonly) {
        $('#tasks-wrapper').on('click', '.actual', this.toggleTaskTimesForm);
        $('#tasks-wrapper').on('change', 'input:checkbox:not(.show_deleted_tasks)', this.toggleTaskStatus);
        $('#tasks-wrapper').on('click', 'td .remaining', this.showEstimateForm);
        $('#tasks-wrapper').on('keydown', 'form.edit_task > input', 'esc', this.hideEstimateForm);
        $('#tasks-wrapper').on('blur', '.remaining_time', this.hideEstimateForm);
      }
      return $('#tasks-wrapper').on('click', '.show_deleted_tasks', this.toggleDeleted);
    },
    toggleTaskTimesForm: function() {
      var current_task_form;
      $('.story-times .actual').not(this).removeClass("selected");
      current_task_form = $(this).toggleClass('selected').closest('tr').next().find('.task_times_form');
      current_task_form.toggle();
      if (current_task_form.is(':visible')) {
        current_task_form.find('textarea').focus();
      }
      return $('.tasksTable .task_times_form').not(current_task_form).hide();
    },
    toggleTaskStatus: function() {
      return $(this).hide().closest('form').submit().prev().show();
    },
    showEstimateForm: function() {
      return $(this).hide().next('form').css('display', 'inline-block').children('input:text').focus();
    },
    hideEstimateForm: function() {
      return $(this).closest('form').hide().prev('.remaining').show();
    },
    toggleDeleted: function() {
      var $h2;
      $h2 = $('#tasks-wrapper').find('h2[class~=deleted]');
      if (!$h2.is(':visible')) {
        $h2.css('display', 'inline-block');
      } else {
        $h2.hide();
      }
      return $('#tasks-wrapper').find('.deleted:not(h2)').toggle();
    }
  };

  $(function() {
    if (/stories\/\d+\/?#?$/.test(window.location.href)) {
      return Tasks.init();
    }
  });

}).call(this);
(function() {
  $(function() {
    $('#tag_filter ul li').live('click', function() {
      $(this).toggleClass('selected');
      App.manager.getTags().filter({
        pushState: true
      });
      return false;
    });
    $('#users_filter ul li').live('click', function() {
      $(this).toggleClass('selected');
      App.manager.getUsers().filter({
        pushState: true
      });
      return false;
    });
    $('#status-filter ul li').live('click', function() {
      $(this).toggleClass('selected');
      App.manager.getStatuses().filter({
        pushState: true
      });
      return false;
    });
    $("#sorted_by a").live('click', function() {
      $('#sorted_by_params span').text("Sort " + ($(this).text()));
      if (!$(this).parent().hasClass('selected')) {
        $(this).parent().addClass('selected').siblings().removeClass('selected');
        App.manager.getSorting().sort();
        App.history.replace(App.manager.url());
      }
      $(document).trigger('menupopup:hide');
      return false;
    });
    $('#sidebar .group li').bind('click', function() {
      if (!$(this).hasClass('selected')) {
        $(this).addClass('selected').siblings().removeClass('selected');
        App.manager.getGrouping();
        App.manager.changeRule({
          pushState: true
        });
      }
      return false;
    });
    $('#users_filter a.clear-filter').live('click', function() {
      $('#users_filter ul li').removeClass('selected');
      App.manager.getUsers().filter({
        pushState: true
      });
      return false;
    });
    $('#tag_filter a.clear-filter').live('click', function() {
      $('#tag_filter ul li').removeClass('selected');
      App.manager.getTags().filter({
        pushState: true
      });
      return false;
    });
    $('#status-filter a.clear-filter').live('click', function() {
      $('#status-filter ul li').removeClass('selected');
      App.manager.getStatuses().filter({
        pushState: true
      });
      return false;
    });
    $("#filter-menu > li").on('click', 'a', function(e) {
      if (e.metaKey) {
        window.open($(this).attr('href'));
        return false;
      }
      if ($(this).hasClass('active')) {
        return false;
      }
      $(this).parent().addClass('active').siblings().removeClass('active');
      App.manager.type = $(this).data('type');
      if (App.manager.isArchive() && !App.itemWallManager.isArchiveReady()) {
        App.itemWallManager.loadArchive();
      } else {
        App.manager.clearFilters();
        App.manager.changeRule({
          pushState: true
        }).updateSidebar();
      }
      return false;
    });
    return $(window).bind('keyup', function(e) {
      if (e.keyCode === 27 && e.target !== $('#search')[0]) {
        return App.manager.clearFilters().filter({
          pushState: true
        });
      }
    });
  });

}).call(this);
(function() {
  $(function() {
    $('.selected-project a').bind("mousedown", function(e) {
      if (e.which === 2) {
        $.cookie("active_project", null, {
          path: '/'
        });
        return window.open("/");
      }
    });
    if (true) {
      $('#project-selector').menuPopup({
        selector: '.projects-list',
        preventEvents: true,
        show: function() {
          $('#project-selector .selected-project').find('.arrow').addClass('active');
          return App.ProjectsSelector.bind();
        },
        hide: function() {
          $('#project-selector .selected-project').find('.arrow').removeClass('active');
          return App.ProjectsSelector.unbind();
        }
      });
    }
    $('.menu-item-selector').menuPopup({
      selector: '.menu-item-list',
      preventEvents: true,
      show: function() {
        $(this).parent().addClass('active');
        return $(this).parent().find('.arrow').addClass('active');
      },
      hide: function() {
        $(this).parent().removeClass('active');
        return $(this).parent().find('.arrow').removeClass('active');
      }
    });
    $('body').on('click', function() {
      $(this).find("span.more").css('color', '#48767B');
      return $(this).find("span.more").css('background', '');
    });
    return $('.projects-list').find('a').bind("click", function(e) {
      var parentWindow;
      $(this).parents('div.projectS-list').trigger('menupopup:hide');
      parentWindow = e.which === 2 || e.metaKey ? true : false;
      return App.helpers.changeProject($(this).data('id'), parentWindow);
    });
  });

}).call(this);
// This [jQuery](http://jquery.com/) plugin implements an `<iframe>`
// [transport](http://api.jquery.com/extending-ajax/#Transports) so that
// `$.ajax()` calls support the uploading of files using standard HTML file
// input fields. This is done by switching the exchange from `XMLHttpRequest`
// to a hidden `iframe` element containing a form that is submitted.

// The [source for the plugin](http://github.com/cmlenz/jquery-iframe-transport)
// is available on [Github](http://github.com/) and dual licensed under the MIT
// or GPL Version 2 licenses.

// ## Usage

// To use this plugin, you simply add an `iframe` option with the value `true`
// to the Ajax settings an `$.ajax()` call, and specify the file fields to
// include in the submssion using the `files` option, which can be a selector,
// jQuery object, or a list of DOM elements containing one or more
// `<input type="file">` elements:

//     $("#myform").submit(function() {
//         $.ajax(this.action, {
//             files: $(":file", this),
//             iframe: true
//         }).complete(function(data) {
//             console.log(data);
//         });
//     });

// The plugin will construct hidden `<iframe>` and `<form>` elements, add the
// file field(s) to that form, submit the form, and process the response.

// If you want to include other form fields in the form submission, include
// them in the `data` option, and set the `processData` option to `false`:

//     $("#myform").submit(function() {
//         $.ajax(this.action, {
//             data: $(":text", this).serializeArray(),
//             files: $(":file", this),
//             iframe: true,
//             processData: false
//         }).complete(function(data) {
//             console.log(data);
//         });
//     });

// ### Response Data Types

// As the transport does not have access to the HTTP headers of the server
// response, it is not as simple to make use of the automatic content type
// detection provided by jQuery as with regular XHR. If you can't set the
// expected response data type (for example because it may vary depending on
// the outcome of processing by the server), you will need to employ a
// workaround on the server side: Send back an HTML document containing just a
// `<textarea>` element with a `data-type` attribute that specifies the MIME
// type, and put the actual payload in the textarea:

//     <textarea data-type="application/json">
//       {"ok": true, "message": "Thanks so much"}
//     </textarea>

// The iframe transport plugin will detect this and pass the value of the
// `data-type` attribute on to jQuery as if it was the "Content-Type" response
// header, thereby enabling the same kind of conversions that jQuery applies
// to regular responses. For the example above you should get a Javascript
// object as the `data` parameter of the `complete` callback, with the
// properties `ok: true` and `message: "Thanks so much"`.

// ### Handling Server Errors

// Another problem with using an `iframe` for file uploads is that it is
// impossible for the javascript code to determine the HTTP status code of the
// servers response. Effectively, all of the calls you make will look like they
// are getting successful responses, and thus invoke the `done()` or
// `complete()` callbacks. You can only determine communicate problems using
// the content of the response payload. For example, consider using a JSON
// response such as the following to indicate a problem with an uploaded file:

//     <textarea data-type="application/json">
//       {"ok": false, "message": "Please only upload reasonably sized files."}
//     </textarea>

// ### Compatibility

// This plugin has primarily been tested on Safari 5 (or later), Firefox 4 (or
// later), and Internet Explorer (all the way back to version 6). While I
// haven't found any issues with it so far, I'm fairly sure it still doesn't
// work around all the quirks in all different browsers. But the code is still
// pretty simple overall, so you should be able to fix it and contribute a
// patch :)

// ## Annotated Source

(function($, undefined) {
  "use strict";

  // Register a prefilter that checks whether the `iframe` option is set, and
  // switches to the "iframe" data type if it is `true`.
  $.ajaxPrefilter(function(options, origOptions, jqXHR) {
    if (options.iframe) {
      return "iframe";
    }
  });

  // Register a transport for the "iframe" data type. It will only activate
  // when the "files" option has been set to a non-empty list of enabled file
  // inputs.
  $.ajaxTransport("iframe", function(options, origOptions, jqXHR) {
    var form = null,
        iframe = null,
        name = "iframe-" + $.now(),
        files = $(options.files).filter(":file:enabled"),
        markers = null,
        accepts;

    // This function gets called after a successful submission or an abortion
    // and should revert all changes made to the page to enable the
    // submission via this transport.
    function cleanUp() {
      markers.prop('disabled', false);
      form.remove();
      iframe.bind("load", function() { iframe.remove(); });
      iframe.attr("src", "javascript:false;");
    }

    // Remove "iframe" from the data types list so that further processing is
    // based on the content type returned by the server, without attempting an
    // (unsupported) conversion from "iframe" to the actual type.
    options.dataTypes.shift();

    if (files.length) {
      form = $("<form enctype='multipart/form-data' method='post'></form>").
        hide().attr({action: options.url, target: name});

      // If there is any additional data specified via the `data` option,
      // we add it as hidden fields to the form. This (currently) requires
      // the `processData` option to be set to false so that the data doesn't
      // get serialized to a string.
      if (typeof(options.data) === "string" && options.data.length > 0) {
        $.error("data must not be serialized");
      }
      $.each(options.data || {}, function(name, value) {
        if ($.isPlainObject(value)) {
          name = value.name;
          value = value.value;
        }
        $("<input type='hidden' />").attr({name:  name, value: value}).
          appendTo(form);
      });

      // Add a hidden `X-Requested-With` field with the value `IFrame` to the
      // field, to help server-side code to determine that the upload happened
      // through this transport.
      $("<input type='hidden' value='IFrame' name='X-Requested-With' />").
        appendTo(form);

      // Borrowed straight from the JQuery source
      // Provides a way of specifying the accepted data type similar to HTTP_ACCEPTS
      accepts = options.dataTypes[ 0 ] && options.accepts[ options.dataTypes[0] ] ?
        options.accepts[ options.dataTypes[0] ] + ( options.dataTypes[ 0 ] !== "*" ? ", */*; q=0.01" : "" ) :
        options.accepts[ "*" ]

      $("<input type='hidden' name='X-Http-Accept'>")
        .attr("value", accepts).appendTo(form);

      // Move the file fields into the hidden form, but first remember their
      // original locations in the document by replacing them with disabled
      // clones. This should also avoid introducing unwanted changes to the
      // page layout during submission.
      markers = files.after(function(idx) {
        return $(this).clone().prop("disabled", true);
      }).next();
      files.appendTo(form);

      return {

        // The `send` function is called by jQuery when the request should be
        // sent.
        send: function(headers, completeCallback) {
          iframe = $("<iframe src='javascript:false;' name='" + name +
            "' id='" + name + "' style='display:none'></iframe>");

          // The first load event gets fired after the iframe has been injected
          // into the DOM, and is used to prepare the actual submission.
          iframe.bind("load", function() {

            // The second load event gets fired when the response to the form
            // submission is received. The implementation detects whether the
            // actual payload is embedded in a `<textarea>` element, and
            // prepares the required conversions to be made in that case.
            iframe.unbind("load").bind("load", function() {
              var doc = this.contentWindow ? this.contentWindow.document :
                (this.contentDocument ? this.contentDocument : this.document),
                root = doc.documentElement ? doc.documentElement : doc.body,
                textarea = root.getElementsByTagName("textarea")[0],
                type = textarea && textarea.getAttribute("data-type") || null,
                status = textarea && textarea.getAttribute("data-status") || 200,
                statusText = textarea && textarea.getAttribute("data-statusText") || "OK",
                content = {
                  html: root.innerHTML,
                  text: type ?
                    textarea.value :
                    root ? (root.textContent || root.innerText) : null
                };
              cleanUp();
              if (!jqXHR.responseText) {
                jqXHR.responseText = content.text;
              }
              completeCallback(status, statusText, content, type ?
                ("Content-Type: " + type) :
                null);
            });

            // Now that the load handler has been set up, submit the form.
            form[0].submit();
          });

          // After everything has been set up correctly, the form and iframe
          // get injected into the DOM so that the submission can be
          // initiated.
          $("body").append(form, iframe);
        },

        // The `abort` function is called by jQuery when the request should be
        // aborted.
        abort: function() {
          if (iframe !== null) {
            iframe.unbind("load").attr("src", "javascript:false;");
            cleanUp();
          }
        }

      };
    }
  });

})(jQuery);



(function($) {

  var remotipart;

  $.remotipart = remotipart = {

    setup: function(form) {
      // Preserve form.data('ujs:submit-button') before it gets nulled by $.ajax.handleRemote
      var button = form.data('ujs:submit-button'),
          csrfParam = $('meta[name="csrf-param"]').attr('content'),
          csrfToken = $('meta[name="csrf-token"]').attr('content'),
          csrfInput = form.find('input[name="' + csrfParam + '"]').length;

      form
        // Allow setup part of $.rails.handleRemote to setup remote settings before canceling default remote handler
        // This is required in order to change the remote settings using the form details
        .one('ajax:beforeSend.remotipart', function(e, xhr, settings){
          // Delete the beforeSend bindings, since we're about to re-submit via ajaxSubmit with the beforeSubmit
          // hook that was just setup and triggered via the default `$.rails.handleRemote`
          // delete settings.beforeSend;
          delete settings.beforeSend;

          settings.iframe      = true;
          settings.files       = $($.rails.fileInputSelector, form);
          settings.data        = form.serializeArray();

          // Insert the name/value of the clicked submit button, if any
          if (button)
            settings.data.push(button);

          // jQuery 1.9 serializeArray() contains input:file entries
          // so exclude them from settings.data, otherwise files will not be sent
          settings.files.each(function(i, file){
            for (var j = settings.data.length - 1; j >= 0; j--)
              if (settings.data[j].name == file.name)
                settings.data.splice(j, 1);
          })

          settings.processData = false;

          // Modify some settings to integrate JS request with rails helpers and middleware
          if (settings.dataType === undefined) { settings.dataType = 'script *'; }
          settings.data.push({name: 'remotipart_submitted', value: true});
          if (csrfToken && csrfParam && !csrfInput) {
            settings.data.push({name: csrfParam, value: csrfToken});
          }

          // Allow remotipartSubmit to be cancelled if needed
          if ($.rails.fire(form, 'ajax:remotipartSubmit', [xhr, settings])) {
            // Second verse, same as the first
            $.rails.ajax(settings);
            setTimeout(function(){ $.rails.disableFormElements(form); }, 20);
          }

          //Run cleanup
          remotipart.teardown(form);

          // Cancel the jQuery UJS request
          return false;
        })

        // Keep track that we just set this particular form with Remotipart bindings
        // Note: The `true` value will get over-written with the `settings.dataType` from the `ajax:beforeSend` handler
        .data('remotipartSubmitted', true);
    },

    teardown: function(form) {
      form
        .unbind('ajax:beforeSend.remotipart')
        .removeData('remotipartSubmitted')
    }
  };

  $(document).on('ajax:aborted:file', 'form', function(){
    var form = $(this);

    remotipart.setup(form);

    // Manually call jquery-ujs remote call so that it can setup form and settings as usual,
    // and trigger the `ajax:beforeSend` callback to which remotipart binds functionality.
    $.rails.handleRemote(form);
    return false;
  });

})(jQuery);
(function() {
  $(function() {
    if ($('#stopProject').attr('data-project') === 'true') {
      return $('#stopProject').show();
    } else if ($('#stopProject').attr('data-project') === 'false') {
      return $('#stopProject').hide();
    }
  });

}).call(this);
$(function () {
  var cookieName = 'submit_option_for_' + $('#current_user_id').val(),
      cookieOption = {path: "/", expires: 3650 },
      initialCookieState = $.cookie(cookieName);

  if ( $('#submit_by_hotkey').length ) {
    $('#submit_by_hotkey').prop('checked', initialCookieState);

    $('#submit_by_hotkey').on('change', function () {
      var $checkbox = $(this);
      $checkbox.is(':checked') ? $.cookie(cookieName, true, cookieOption) : $.cookie(cookieName, null, cookieOption);
    });
  }

  $('body').on('keydown', 'form textarea', 'ctrl+return', function () {
    if ( $.cookie(cookieName) ) {
      $(this).closest('form').submit();
    }
  });
  $('body').on('keydown', 'form textarea', 'meta+return', function () {
    if ( $.cookie(cookieName) ) {
      $(this).closest('form').submit();
    }
  });
})
;
$(function () {
    $("#project_with_contract").click(function () {
        if ($(this).is(":checked")) {
            $(".hidden_attributes").show();
        } else {
            $(".hidden_attributes").hide();
        }
    });
});

