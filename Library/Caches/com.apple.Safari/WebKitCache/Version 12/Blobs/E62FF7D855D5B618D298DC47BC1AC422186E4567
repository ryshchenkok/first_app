(function() {
  (function($) {
    return $.fn.upload = function(settings) {
      var $this, bind, dragEnter, dragOver, drop, formData, getToken, send, xhr;
      $this = this;
      formData = new FormData();
      xhr = new XMLHttpRequest();
      settings = $.extend({
        url: location.href + "/attachments.js",
        dropClass: "file-drop",
        text: "Drop your files here!",
        before: null,
        after: null,
        progress: null
      }, settings || {});
      if (!!window.FormData) {
        bind = function() {
          $this.bind("dragenter.upload", dragEnter);
          $this.bind("dragover.upload", dragOver);
          return $this.bind("drop.upload", drop);
        };
        dragEnter = function(event) {
          event.stopPropagation();
          event.preventDefault();
          return false;
        };
        dragOver = function(event) {
          event.stopPropagation();
          event.preventDefault();
          return false;
        };
        send = function() {
          var token;
          token = getToken();
          xhr.open("POST", settings.url);
          xhr.overrideMimeType('text/javascript');
          if (token) {
            xhr.setRequestHeader("X-CSRF-Token", token);
          }
          xhr.onreadystatechange = function() {
            if (xhr.readyState === 4 && xhr.status === 200) {
              return eval(xhr.responseText);
            }
          };
          xhr.send(formData);
          return false;
        };
        drop = function(e) {
          var f, files, i, ref;
          e.stopPropagation();
          e.preventDefault();
          files = e.originalEvent.target.files || e.originalEvent.dataTransfer.files;
          ref = [0, void 0], i = ref[0], f = ref[1];
          while (f = files[i]) {
            formData.append("file_attachment[]", f);
            i++;
          }
          send();
          formData = new FormData();
          return false;
        };
        getToken = function() {
          return $("meta[name=\"csrf-token\"]").attr("content");
        };
        bind();
      }
      return this;
    };
  })(jQuery);

}).call(this);
