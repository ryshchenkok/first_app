    <script type="text/javascript">
  var dateEl = document.querySelector('span[itemProp=dateModified]');
  var lastModifiedEl = document.querySelector('span[itemProp=lastModified]');
  if(dateEl) {
    dateEl.innerText = new Date(dateEl.innerText).toLocaleDateString('ru-ru', {
      year: 'numeric',
      month: 'long',
      day: 'numeric'
    })
  }
  if(lastModifiedEl) {
  lastModifiedEl.innerText = new Date(lastModifiedEl.innerText).
    toLocaleDateString('ru-ru', {
      year: 'numeric',
      month: 'long',
      day: 'numeric'
    })
}
</script>
    
